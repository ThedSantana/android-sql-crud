<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="qodr.sqlitecrud">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DeleteActivity"
            android:noHistory="true"/>
        <activity android:name=".activity.CreateActivity" android:noHistory="true" />
        <activity android:name=".activity.ReadActivity" android:noHistory="true" />
        <activity android:name=".activity.UpdateActivity" android:noHistory="true"/>
    </application>

</manifest>